<odoo>
    <data>
        <record model="ir.ui.view" id="project_cemeteries_list_view">
            <field name="name">project.cemeteries.list.view</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="Cemeteries">
                        <field name="cemeteries" nolabel="1">
                            <tree editable="top">
                                <field name="name"/>
                                <field name="location"/>
                                <field name="date"/>
                                <!-- Add any other fields you need here -->
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
