<odoo>
    <data>
        <record model="ir.ui.view" id="project_view_notebook_inherited">
            <field name="name">project.project.view.form.notebook.inherited</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">
                    <notebook position='inside'>
                        <page string="Cemeteries">
                        <group>
                            <field name="cemetery_ids" >
                                <tree editable="bottom">
                                    <field name="name"/>
                                    <field name="location"/>
                                    <field name="date"/>
                                </tree>
                            </field>
                        </group>
                        </page>
                    </notebook>
            </field>
        </record>
    </data>
</odoo> 
